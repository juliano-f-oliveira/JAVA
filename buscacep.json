{
    "request":{
        "method": "GET",
        "urlPattern": "/cep/.*"
    },

    "response":{
        "status":200,
        "jsonBody":{"localidade":"MOCK","uf":"SP","cep":"{{request.pathSegments.[1]}}"},
        "headers":{
            "Content-type":"application/json"
        }

    }
}